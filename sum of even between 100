#include <stdio.h>

int main() {
    int sum = 0;

    // Loop through numbers from 1 to 100
    for (int i = 1; i <= 100; i++) {
        // Check if the number is even
        if (i % 2 == 0) {
            sum += i; // Add the even number to the sum
        }
    }

    // Print the result
    printf("The sum of all even numbers between 1 and 100 is: %d\n", sum);

    return 0;
}
