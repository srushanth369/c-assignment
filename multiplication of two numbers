#include <stdio.h>
int main() {    

    int number1, number2, multiplication;
    
    printf("Enter two integers: ");
    scanf("%d %d", &number1, &number2);

    // calculate the multiplication
   multiplication = number1 + number2;      
    
    printf("%d + %d = %d", number1, number2, multiplication);
    return 0;
}
