#include <stdio.h>

int main() {
    int num1, num2;

    // Input two numbers from user
    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);

    // Compare num1 with num2
    if (num1 > num2) {
        printf("%d is maximum\n", num1);
    } else if (num2 > num1) {
        printf("%d is maximum\n", num2);
    } else {
        printf("Both numbers are equal\n");
    }

    return 0;
}
