#include <stdio.h>

void removeDuplicates(int arr[], int *n) {
    int temp[*n]; // Temporary array to store unique elements
    int j = 0;

    for (int i = 0; i < *n; i++) {
        int isDuplicate = 0;
        for (int k = 0; k < j; k++) {
            if (arr[i] == temp[k]) {
                isDuplicate = 1; // Found a duplicate
                break;
            }
        }
        if (!isDuplicate) {
            temp[j] = arr[i]; // Add unique element to temp
            j++;
        }
    }

    // Copy unique elements back to original array
    for (int i = 0; i < j; i++) {
        arr[i] = temp[i];
    }

    *n = j; // Update the size of the original array
}

void printArray(int arr[], int n) {
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");
}

int main() {
    int arr[] = {1, 2, 2, 3, 4, 4, 5, 5, 5};
    int n = sizeof(arr) / sizeof(arr[0]);

    printf("Original array: \n");
    printArray(arr, n);

    removeDuplicates(arr, &n);

    printf("Array after removing duplicates: \n");
    printArray(arr, n);

    return 0;
}
