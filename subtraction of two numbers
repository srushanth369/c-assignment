#include <stdio.h>
int main() {    

    int number1, number2, subtraction;
    
    printf("Enter two integers: ");
    scanf("%d %d", &number1, &number2);

    // calculate the subtraction
   subtraction = number1 + number2;      
    
    printf("%d + %d = %d", number1, number2, subtraction);
    return 0;
}
